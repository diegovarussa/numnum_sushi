function draw_belt()
	if time() - animation_time > wait_time then
		sprite+=1
		animation_time=time()
		if sprite > 11 then
			sprite=4
		end
	end
	
	for i=0,16 do
		spr(sprite,belt.x+i*8,belt.y)
	end
end

function draw_sushi()
	if sushi.x > 127 then
		sushi.x=0
	end
	
	sushi.x+=sushi.speed
	spr(sushi.sprite,sushi.x,sushi.y,sushi.size, sushi.size)	
end

